{
  "compilerOptions": {
    "importHelpers": true,
    "declaration": true,
    "declarationMap": true,
    "alwaysStrict": true,
    "sourceMap": true,
    "target": "esnext",
    "lib": ["ES2021.String"],
    "esModuleInterop": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "module": "esnext",
    "moduleResolution": "node",
    "isolatedModules": true,
    "strictNullChecks": true,
    "stripInternal": true,
    "noFallthroughCasesInSwitch": false,
    "noEmitOnError": false,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
